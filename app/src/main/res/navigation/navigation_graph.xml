<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    android:label="@string/string_accueil"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="fr.doranco.projetfinal.fragment.HomeFragment"
        android:label="@string/string_accueil"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_paysListFragment"
            app:destination="@id/paysListFragment" />
    </fragment>
    <fragment
        android:id="@+id/paysListFragment"
        android:name="fr.doranco.projetfinal.fragment.PaysListFragment"
        android:label="@string/string_pays"
        tools:layout="@layout/fragment_pays_list" />
</navigation>